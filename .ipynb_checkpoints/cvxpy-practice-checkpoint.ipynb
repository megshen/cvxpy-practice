   "source": [
    "import cvxpy as cp\n",
    "x = cp.Variable()\n",
    "constraints = [x >= 0.5]\n",
    "obj = cp.Maximize(-(x**2)+1)\n",
    "\n",
    "prob = cp.Problem(obj, constraints)\n",
    "prob.solve()  # Returns the optimal value.\n",
    "print(\"status:\", prob.status)\n",
    "print(\"optimal value\", prob.value)\n",
    "print(\"optimal var\", x.value)"
   ]
  }
